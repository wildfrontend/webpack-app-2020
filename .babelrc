{
    "presets": ["@babel/preset-env", "@babel/preset-react"],
    "plugins": [
        [
            "module-resolver",
            {
                "root": ["."],
                "alias": {
                    "routes": "./src/routes",
                    "pages": "./src/pages",
                    "components": "./src/components",
                    "assets": "./src/assets",
                    "reducers": "./src/store/reducers"
                }
            }
        ]
    ]
}
